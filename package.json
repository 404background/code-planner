    "stl-test": "python ./python/stl_test.py",
    "plugin-test": "python ./python/plugin_test.py",
    "arduino-creater": "python ./python/arduino_creater.py",
    "node-red": "start node ./node-red.js",
    "win-jupyter": "start /d windows jupyter-lab.bat",
